<section class='section'>
  <%- @faqs.each do |faq| %>
    <div class='box'>
      <strong><%= faq.question %></strong>
      <%= faq.answer %>
      <div class='actions'>
        <%= link_to 'Edit', edit_faq_path(faq) %>
        <%= link_to 'Delete', faq_path(faq), method: :delete %>
      </div>
    </div>
  <% end %>
</section>
